---
import { YouTube } from "astro-embed";
import { type YouTubeVideoElementModel } from "@/api";

const { data } = Astro.props;
const typedData = data as YouTubeVideoElementModel;
const videoId = typedData?.properties?.videoID;
---

<figure
  class="lg:-mr-32 xl:-mr-48 2xl:-mr-60 3xl:-mr-72 rounded-2xl overflow-hidden"
>
  <YouTube id={videoId} style="width: 100%;" />
</figure>
